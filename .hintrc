{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "disown-opener": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "axe/name-role-value": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not firefox <= 138",
    "not safari <= 18.4",
    "not ios_saf <= 18.4"
  ]
}