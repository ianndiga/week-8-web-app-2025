{
  "name": "jijue-hospital-fullstack",
  "version": "1.0.0",
  "description": "Jijue Hospital - Fullstack Healthcare Application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "fullstack": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon server.js",
    "dev:frontend": "cd frontend && npx http-server -p 3000 -c-1 -o",
    "start": "node server.js",
    "start:frontend": "cd frontend && npx http-server -p 3000 -c-1 -o",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "test": "jest",
    "test:backend": "jest server.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.4.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "hospital",
    "healthcare",
    "medical",
    "fullstack",
    "express",
    "mongodb",
    "react"
  ],
  "author": "Jijue Hospital",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "your-repository-url"
  }
}